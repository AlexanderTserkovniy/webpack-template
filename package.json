{
  "name": "webpack-template",
  "version": "0.0.1",
  "description": "Webpack 2 template to start from. Contains React + HMR, Babel, PostCSS",
  "main": "index.js",
  "scripts": {
    "index-html": "cp ./templates/index.html ./dist/index.html",
    "start": "npm run index-html && webpack-dev-server",
    "build": "NODE_ENV=production webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderTserkovniy/webpack-template.git"
  },
  "keywords": [
    "webpack2",
    "template"
  ],
  "author": "Oleksandr Tserkovnyi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlexanderTserkovniy/webpack-template.git"
  },
  "homepage": "https://github.com/AlexanderTserkovniy/webpack-template.git#readme",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "postcss-loader": "^0.9.1",
    "react-css-modules": "^4.1.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.7",
    "webpack-dev-server": "^2.2.0-rc.0"
  }
